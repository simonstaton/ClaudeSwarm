project_id        = "your-gcp-project-id"
region            = "europe-west2"
openrouter_api_key = "sk-or-v1-..."
agent_api_key     = "your-login-key-here"
image             = "europe-west2-docker.pkg.dev/your-project/claude-swarm/claude-swarm:latest"

# Cloud Run access — who can invoke the service
# Use ["allUsers"] for public access (app has its own JWT auth)
# Or ["user:you@example.com"] for IAM-restricted access
cloud_run_invokers = ["allUsers"]

# GitHub Actions CI/CD — set to enable Workload Identity Federation
# github_repo = "your-org/your-repo"

# Cloud Monitoring alerts — set an email to enable all alert policies
# Alerts: high error rate, high latency, crashes, memory >80%, CPU >80%
# alert_notification_email = "ops@example.com"

# Optional MCP credentials — uncomment and fill in to enable
# github_token        = "ghp_..."
# notion_api_key      = "ntn_..."
# slack_token         = "xoxb-..."
# google_credentials  = "..."
